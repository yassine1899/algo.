ALGORITHM produit_scalair
VAR
    ps,i,j:INTEGER
    t1,t2 : array_of FLOAT;
BEGIN
    FOR i FROM 0 TO t1.length STEP step 1 DO
      Read (t1[i]);
    END_FOR
     FOR j FROM 0 TO t2.length STEP step 1 DO
        -Read (t2[j]);
     END_FOR
     FOR i FROM 0 TO t1.length STEP 1 DO
        ps:= ps + t1[i]*t2[i];
     END_FOR
     IF (ps = 0) THEN
       Write("t1 et t2 sont orthogonaux")
       ELSE
       Write("t1 et t2 sont orthogonaux")
     END_IF
END